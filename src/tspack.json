{
  "compilerOptions": {
    "outFile": "../lib/depsync.js",
    "target": "ES5",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "skipDefaultLibCheck": true,
    "stripInternal": true,
    "accessorOptimization": true,
    "emitReflection": true,
    "reorderFiles": true
  }
}
